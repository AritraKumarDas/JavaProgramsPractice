
class SuperClass{
	
	int x=10;
//	SuperClass(int x){
//		this.x = x;
//	}
	public void display() {
		System.out.println("x = " + x);
	}
	
}

class BaseClass extends SuperClass{
	int y=20;
	
//	BaseClass(int x, int y){
//		super(x);
//		this.y = y;
//	}
	public void display() {
//		super.display();
		System.out.println("y = " + y);
	}
	
}

public class MainClass {

	public static void main(String[] args) {
		
		SuperClass obj = new BaseClass();
		
		obj.display();
	}
}
