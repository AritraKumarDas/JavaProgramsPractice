package inheritance;

class InheritanceA {

	private float circleArea(float radius){
		
		return (22/7 * radius * radius);	
	}
}

class InheritanceB extends InheritanceA {

	private float rectangleArea(float length, float breadth){
		
		return (length * breadth);	
	}
}

class InheritanceC extends InheritanceB{

	private float triangleArea(float base, float height){
		
		return (1/3 * base * height);	
	}
}

public class Main{
	public static void main(String[] args) {
		
		InheritanceC obj = new InheritanceC();
		float circlearea = obj.circleArea(7.5f);
	}
}




